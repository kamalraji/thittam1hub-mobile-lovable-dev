    "date-fns": "^3.6.0",
    "dompurify": "^3.3.1",
    "embla-carousel-react": "^8.6.0",
    "fabric": "^6.9.1",
    "framer-motion": "^12.23.26",
    "grapesjs": "^0.22.14",
    "html2canvas": "^1.4.1",
    "jspdf": "^2.5.2",
    "lucide-react": "^0.462.0",
    "next-themes": "^0.3.0",
    "react": "^18.3.1",
    "react-day-picker": "^8.10.1",
