{
  "name": "thittam1hub-frontend",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "node build-integration.js && tsc && vite build",
    "build:dev": "vite build --mode development",
    "build:prod": "NODE_ENV=production npm run build",
    "build:fast": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "type-check": "tsc --noEmit",
    "design-system:check": "npm run type-check && npm run lint && npm run format:check",
    "validate-config": "node validate-config.js",
    "build-integration": "node build-integration.js",
    "analyze-deps": "node analyze-deps.js",
    "prebuild": "npm run validate-config"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@hookform/resolvers": "^3.10.0",
